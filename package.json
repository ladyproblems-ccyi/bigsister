{
  "name": "bigsister",
  "version": "0.0.1",
  "description": "Mobile optmized UI for ladyproblems hackathon.",
  "options": {
    "mocha": "tools/testSetup.js src/**/*.spec.js --reporter progress"
  },
  "engines": {
    "node": "6.3.1",
    "npm": "3.10.3"
  },
  "scripts": {
    "prestart": "npm run buildonly",
    "start": "./node_modules/.bin/babel-node productionRequire.js",
    "start-message": "./node_modules/.bin/babel-node tools/startMessage.js",
    "prestartdev": "npm-run-all --parallel start-message remove-dist",
    "startdev": "npm-run-all --parallel prestartdev test:watch open:src",
    "lint": "esw webpack.config.* src tools --color --ignore-pattern node_modules; exit 0;",
    "lint:watch": "esw webpack.config.* src tools --color --ignore-pattern node_modules --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "open:src": "./node_modules/.bin/babel-node tools/srcServer.js",
    "open:dist": "./node_modules/.bin/babel-node tools/distServer.js",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "npm run buildonly",
    "buildonly": "./node_modules/.bin/babel-node tools/build.js",
    "buildAndRun": "./node_modules/.bin/babel-node tools/build.js && npm run open:dist",
    "testonly": "./node_modules/.bin/_mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ladyproblems-ccyi/bigsister.git"
  },
  "keywords": [
    "culture",
    "women",
    "community",
    "sister",
    "sisterhood"
  ],
  "author": "Charles, Iga, Ylenia, Claudia",
  "license": "UNLICENSED",
  "homepage": "",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.9.0",
    "browser-sync": "2.13.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "npm-run-all": "2.3.0",
    "path": "^0.12.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-fb-auth": "^1.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "uuid": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "^6.4.1",
    "eslint-watch": "^2.1.14",
    "faker": "^3.1.0",
    "mocha": "^3.1.2",
    "radium": "^0.18.1",
    "ramda": "^0.23.0",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.6",
    "uuid": "^2.0.3"
  }
}
